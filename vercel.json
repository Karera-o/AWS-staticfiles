{
    "builds": [{
      "src": "core/wsgi.py",
      "use": "@vercel/python",
      "config": {
        "runtime": "python3.9",
        "config": { "maxLambdaSize": "15mb" }
      }
      
    },
    {
      "src": "build_files.sh",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "staticfiles"
      }
    
    }],
    
    "routes": [{
      "src": "/(.*)",
      "dest": "core/wsgi.py"
    }],
    "outputDirectory":"staticfiles"
  }